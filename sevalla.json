{
  "name": "cncf-issue-tracker",
  "version": "2.0.0",
  "description": "CNCF Projects Issue Tracker Bot with Real-time Webhooks",
  "start": "python cncf_issue_tracker_webhook.py",
  "buildCommand": "pip install -r requirements.txt",
  "healthCheckPath": "/health",
  "restart": {
    "policy": "on-failure",
    "maxRetries": 10
  },
  "environment": {
    "PORT": "8080",
    "LOG_LEVEL": "INFO"
  }
}
